{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build",
    "lint": "next lint",
    "pages:build": "bunx next-on-pages",
    "pages:deploy": "bun run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "bunx next-on-pages --watch",
    "pages:dev": "bunx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@tailwindcss/typography": "0.5.12",
    "@tinacms/cli": "1.5.44",
    "@types/node": "20.12.7",
    "@types/react": "18.3.0",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "cheerio": "1.0.0-rc.12",
    "clsx": "2.1.1",
    "feed": "4.2.2",
    "next": "14.2.3",
    "next-themes": "0.3.0",
    "postcss": "8.4.38",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "sharp": "0.33.3",
    "stream": "0.0.2",
    "tailwindcss": "3.4.3",
    "tinacms": "1.6.2",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.11.1",
    "eslint": "9.1.1",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next-on-pages": "1.11.1",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "vercel": "34.1.3",
    "wrangler": "3.52.0"
  }
}
