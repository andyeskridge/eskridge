{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "lint": "next lint",
    "pages:build": "bunx next-on-pages",
    "pages:deploy": "bun run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "bunx next-on-pages --watch",
    "pages:dev": "bunx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@mapbox/rehype-prism": "0.9.0",
    "@mdx-js/loader": "3.0.0",
    "@mdx-js/react": "3.0.0",
    "@next/mdx": "14.0.3",
    "@tailwindcss/typography": "0.5.10",
    "@tinacms/cli": "1.5.38",
    "@types/node": "20.10.3",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "cheerio": "1.0.0-rc.12",
    "clsx": "2.0.0",
    "fast-glob": "3.3.2",
    "feed": "4.2.2",
    "next": "14.0.3",
    "next-themes": "0.2.1",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-gfm": "4.0.0",
    "sharp": "0.33.0",
    "stream": "0.0.2",
    "tailwindcss": "3.3.6",
    "tinacms": "1.5.28",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.8.1",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next-on-pages": "1.8.1",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.8",
    "vercel": "32.6.1",
    "wrangler": "3.19.0"
  }
}