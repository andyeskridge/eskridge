{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "bun build:worker && bun dev:worker",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build",
    "lint": "next lint",
    "cf-typegen": "wrangler types --experimental-include-runtime --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@tailwindcss/typography": "0.5.16",
    "@tinacms/cli": "1.8.0",
    "@types/node": "22.10.7",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.20",
    "cheerio": "1.0.0",
    "clsx": "2.1.1",
    "feed": "4.2.2",
    "next": "15.1.4",
    "next-themes": "0.4.4",
    "postcss": "8.5.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.33.5",
    "stream": "0.0.3",
    "tailwindcss": "3.4.17",
    "tinacms": "2.6.0",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@opennextjs/cloudflare": "0.3.8",
    "@playwright/test": "1.49.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "10.0.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.10",
    "wrangler": "3.102.0"
  },
  "overrides": {
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3"
  }
}
