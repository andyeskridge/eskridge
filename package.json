{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:worker": "opennextjs-cloudflare build",
    "build": "tinacms build && next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev:worker": "opennextjs-cloudflare preview -- --port 8771",
    "dev": "tinacms dev -c \"next dev\"",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "lint:ci": "biome ci",
    "lint": "biome check",
    "populate:local": "opennextjs-cloudflare populateCache local",
    "populate:remote": "opennextjs-cloudflare populateCache remote",
    "preview:worker": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.8",
    "cheerio": "1.1.2",
    "clsx": "2.1.1",
    "feed": "5.1.0",
    "next": "15.5.3",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tinacms": "2.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@opennextjs/aws": "3.7.7",
    "@opennextjs/cloudflare": "1.8.2",
    "@playwright/test": "1.55.0",
    "@tailwindcss/postcss": "4.1.13",
    "@tailwindcss/typography": "0.5.16",
    "@tinacms/cli": "1.10.3",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "ultracite": "5.1.2",
    "wrangler": "4.37.1"
  },
  "overrides": {
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide"
  ]
}
