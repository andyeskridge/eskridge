{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:worker": "opennextjs-cloudflare build",
    "build": "tinacms build && next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev:worker": "opennextjs-cloudflare preview -- --port 8771",
    "dev": "tinacms dev -c \"next dev\"",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "lint:ci": "biome ci",
    "lint": "biome check",
    "populate:local": "opennextjs-cloudflare populateCache local",
    "populate:remote": "opennextjs-cloudflare populateCache remote",
    "preview:worker": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.4",
    "cheerio": "1.1.0",
    "clsx": "2.1.1",
    "feed": "5.1.0",
    "next": "15.3.3",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tinacms": "2.7.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@opennextjs/aws": "3.6.5",
    "@opennextjs/cloudflare": "1.2.1",
    "@playwright/test": "1.53.0",
    "@tailwindcss/postcss": "4.1.8",
    "@tailwindcss/typography": "0.5.16",
    "@tinacms/cli": "1.9.8",
    "@types/node": "24.0.0",
    "@types/react-dom": "19.1.6",
    "@types/react": "19.1.8",
    "postcss": "8.5.4",
    "tailwindcss": "4.1.8",
    "typescript": "5.8.3",
    "ultracite": "4.2.10",
    "wrangler": "4.19.2"
  },
  "overrides": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
