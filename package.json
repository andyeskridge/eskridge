{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build",
    "lint": "next lint",
    "pages:build": "bunx next-on-pages",
    "pages:deploy": "bun run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "bunx next-on-pages --watch",
    "pages:dev": "bunx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@headlessui/react": "2.0.4",
    "@tailwindcss/typography": "0.5.13",
    "@tinacms/cli": "1.5.45",
    "@types/node": "20.14.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "cheerio": "1.0.0-rc.12",
    "clsx": "2.1.1",
    "feed": "4.2.2",
    "next": "14.2.4",
    "next-themes": "0.3.0",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.33.4",
    "stream": "0.0.2",
    "tailwindcss": "3.4.4",
    "tinacms": "1.6.4",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.11.3",
    "eslint": "9.5.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next-on-pages": "1.11.3",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.5",
    "vercel": "34.2.7",
    "wrangler": "3.60.3"
  }
}
