{
  "name": "eskridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:worker": "opennextjs-cloudflare build",
    "build": "tinacms build && next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev:worker": "opennextjs-cloudflare preview -- --port 8771 --ip 0.0.0.0",
    "dev": "tinacms dev -c \"next dev\"",
    "pree2e": "node scripts/playwright-docker.mjs",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "lint:ci": "biome ci",
    "lint": "biome check",
    "populate:local": "opennextjs-cloudflare populateCache local",
    "populate:remote": "opennextjs-cloudflare populateCache remote",
    "preview:worker": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "tinacms build && next start",
    "tina:build": "tinacms build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "clsx": "2.1.1",
    "feed": "5.1.0",
    "next": "15.5.6",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tinacms": "2.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@opennextjs/cloudflare": "1.11.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "4.1.14",
    "@tailwindcss/typography": "0.5.19",
    "@tinacms/cli": "1.11.0",
    "@types/node": "24.8.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3",
    "ultracite": "5.6.4",
    "wrangler": "4.43.0"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide"
  ]
}
